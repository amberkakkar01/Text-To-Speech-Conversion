   "source": [
    "from gtts import gTTS\n",
    "import os\n",
    "\n",
    "myText = \"Hello I'm Amber, this is my first text to speech program using python\"\n",
    "\n",
    "language = 'en'\n",
    "\n",
    "output = gTTS(text=myText, lang=language, slow=False)\n",
    "\n",
    "output.save(\"output.mp3\")\n",
    "os.system(\"start output.mp3\")\n"
   ]
  }
